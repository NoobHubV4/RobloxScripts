local lib = loadstring(Game:HttpGet("https://raw.githubusercontent.com/NoobHubV1/NoobHubV1/main/Wizard%20Lib.lua"))()
local Rstorage = game.ReplicatedStorage
local autoMoon
local enchant = "Rare"
local Window = lib:NewWindow("Create a Factory")
local Main = Window:NewSection("Main")
local NPC = Window:NewSection("NPC")
Main:CreateTextbox("Give Money", function(text)
  for i = 1,4 do
    Rstorage.SetupConveyorTouches:FireServer(workspace:FindFirstChild("Base"..i).PlacedPieces.Seller, tonumber(text), 0)
  end
end)
Main:CreateToggle("Auto Moon Coin", function(state)
  autoMoon = state
end)
NPC:CreateDropdown("Enchant", {"Rare","Legendary","Mythical"}, 1, function(select)
  enchant = select
end)
NPC:CreateButton("Enchant All NPC", function()
  for i = 1,4 do
    for i,v in next, game.workspace:FindFirstChild("Base"..i):GetChildren() do
      if v.Name:lower():find("npc_") then
        Rstorage.NPCDecision:FireServer("Orb", v, enchant)
      end
    end
  end
end)
NPC:CreateButton("Drop All NPC", function()
  for i = 1,4 do
    for i,v in next, game.workspace:FindFirstChild("Base"..i):GetChildren() do
      if v.Name:lower():find("npc_") then
        Rstorage.Drop:FireServer("Orb", v, "Mythical")
      end
    end
  end
end)
spawn(function()
  function task0()
    if autoMoon then
      Rstorage.GiveMoonCoin:FireServer()
    end
  end
  while task.wait() do
    task.spawn(task0)
  end
end)
