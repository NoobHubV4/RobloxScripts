local lib = loadstring(Game:HttpGet("https://raw.githubusercontent.com/NoobHubV1/NoobHubV1/main/Wizard%20Lib.lua"))()
local autoRoll
local remove
local Window = lib:NewWindow("Mount RNG")
local Section = Window:NewSection("Main")
Section:CreateToggle("Auto Roll", function(state)
  autoRoll = state
end)
spawn(function()
  task0 = function()
    if autoRoll then
      game.ReplicatedStorage.Remotes.RollFunction:InvokeServer()   
    end
  end
  while task.wait() do
    pcall(task0)
  end
end)
