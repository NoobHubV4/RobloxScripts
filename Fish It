local lib = loadstring(Game:HttpGet("https://raw.githubusercontent.com/NoobHubV1/NoobHubV1/main/Wizard%20Lib.lua"))()
local remote = game.ReplicatedStorage.Packages:FindFirstChild("_Index"):FindFirstChild("sleitnick_net@0.2.0").net
local Window = lib:NewWindow("Fish It")
local Section = Window:NewSection("Main")
Section:CreateButton("Throw Perfect", function()
  remote:FindFirstChild("RE/EquipToolFromHotbar"):FireServer(1); wait(.1)
  remote:FindFirstChild("RF/ChargeFishingRod"):InvokeServer(0)
  remote:FindFirstChild("RF/RequestFishingMinigameStarted"):InvokeServer(0, 1)
end)
Section:CreateButton("God Mode", function()
  remote:FindFirstChild("URE/UpdateOxygen"):FireServer(-math.huge)
end)
spawn(function()
  function task0()
    remote:FindFirstChild("RE/FishingCompleted"):FireServer()
  end
  while task.wait() do
    pcall(task0)
  end
end)
