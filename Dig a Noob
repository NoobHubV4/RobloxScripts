local lib = loadstring(Game:HttpGet("https://raw.githubusercontent.com/NoobHubV1/NoobHubV1/main/Wizard%20Lib.lua"))()
local autoDig
local Window = lib:NewWindow("Dig a Noob")
local args = {
	0,
	0.5,
	0,
	"Xr7!pLq9@Vm2#"
}
local Section = Window:NewSection("Main")
Section:CreateToggle("Auto Dig", function(state)
  autoDig = state
end)
spawn(function()
  task0 = function()
    if autoDig then
      game.ReplicatedStorage.DigEvent:FireServer(unpack(args))
    end
  end
  while task.wait() do
    pcall(task0)
  end
end)
