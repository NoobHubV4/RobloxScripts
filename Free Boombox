UIGradient.Parent = TextLabel2
task.spawn(function()
	while game do
		task.wait()
		UIGradient.Rotation +=.005
	end
end)

local Players = game:GetService("Players")

local localPlayer = Players.LocalPlayer
local Amount
local dupesDone = 0
local active = nil
local auto = nil

function missing(t, f, fallback)
    if type(f) == t then return f end
    return fallback
end

replicatesignal = missing("function", replicatesignal)

function Notif(Title, Text, Time)
  local Time = Time
  if not Time then
        Time = 3
  end
  game:GetService("StarterGui"):SetCore("SendNotification", {Title = Title, Text = Text, Duration = Time,})
end

function FindRadioTool()
  local character, backpack = localPlayer.Character:GetChildren(), localPlayer.Backpack:GetChildren()
    for i,v in ipairs(backpack) do
            if v:IsA("Tool") and v.Name == "BoomBox" then
                    return v
            end
    end
    for i,v in pairs(character) do
            if v:IsA("Tool") and v.Name == 'BoomBox' then
                    return v
            end
    end
end

function Dupe(Amount)
 if Amount ~= nil then
    if localPlayer.Character.Humanoid.Health ~= 0 then
      if not active then
  active = true
  auto = true
  TextLabel2.Visible = true
  TextLabel2.Text = "Dupe: "..dupesDone.." time"
  while auto do
        if dupesDone >= Amount then
            dupesDone = 0
            Amount = 0
            active = false
            auto = false
            wait(.325)
            TextLabel2.Visible = false
            break
        end
        if not FindRadioTool() then
                Notif("Error", 'Player not BoomBox auto respawn')
                replicatesignal(localPlayer.ConnectDiedSignalBackend)
                wait(game.Players.RespawnTime - 0.3)
                replicatesignal(localPlayer.Kill)
                localPlayer.CharacterAdded:Wait() wait(.2)
        end
        local char = localPlayer.Character
        localPlayer.Character = nil
        localPlayer.Character = char
        char.Animate:Destroy()
        localPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(0, 10000, 0)
        wait(.1)
        localPlayer.Character.HumanoidRootPart.Anchored = true
        char.Humanoid:UnequipTools()
        replicatesignal(localPlayer.ConnectDiedSignalBackend)
        wait(Players.RespawnTime - 0.3)
        for _, tool in pairs(localPlayer.Backpack:GetChildren()) do
            if tool:IsA("Tool") then
                tool.Parent = char
		tool.Parent = workspace
            end
        end
        replicatesignal(localPlayer.Kill)

         localPlayer.CharacterAdded:Wait()
         wait(.1)
         local newChar = localPlayer.Character or localPlayer.CharacterAdded:Wait()
         for _, item in ipairs(workspace:GetChildren()) do
             pcall(function()
                 newChar.Humanoid:EquipTool(item)
             end)
         end
         dupesDone = dupesDone + 1
         TextLabel2.Text = "Dupe: "..dupesDone.." time"
         wait(.3)
    end
    elseif active then
         Notif("Error", "Script Active, wait a end", 5)
    end
 else
    Notif("Error", "Player Dead", 5)
 end
else
  Notif("Error", "Not Amount", 5)
end
end

TextButton.MouseButton1Click:Connect(function()
amount = tonumber(TextBox.Text)
Dupe(amount)
end)
TextButton2.MouseButton1Click:Connect(function()
    local character = localPlayer.Character or localPlayer.CharacterAdded:Wait()
    local tool = character:FindFirstChildOfClass("Tool")
    if tool then
        tool.Parent = workspace
        Notif("Successfully", "Drop "..tool.Name)
    end
end)
