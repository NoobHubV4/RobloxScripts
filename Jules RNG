local lib = loadstring(Game:HttpGet("https://raw.githubusercontent.com/NoobHubV1/NoobHubV1/main/Wizard%20Lib.lua"))()
local autoRoll
local remove
local blur
local Window = lib:NewWindow("Jule's RNG")
local Section = Window:NewSection("Main")
function createBlur()
  blur = Instance.new("BlurEffect", game.Lighting)
  blur.Name = "Blur"
  blur.Enabled = true
  blur.Size = 0
end
Section:CreateToggle("Auto Roll", function(state)
  autoRoll = state
  if autoRoll then
    game.Players.LocalPlayer.PlayerGui:FindFirstChild("Fullscreen_RollingUI").Roll.Visible = falde
    if blur then
      blur:Destroy()
      blur = nil
    end
    remove = true
  else
    game.Players.LocalPlayer.PlayerGui:FindFirstChild("Fullscreen_RollingUI").Roll.Visible = true
    createBlur()
    remove = false
  end
end)
spawn(function()
  task0 = function()
    if autoRoll then
      game.ReplicatedStorage.Remotes.Roll:FireServer(buffer.fromstring("\128\000\000\000u\196;\253\001\000\000\000\000\000\000 \220\191\025@"))   
    end
    if remove then
      if not game.Players.LocalPlayer.PlayerGui:FindFirstChild("Core_RollingButtons").Frame.Roll.Button.Visible then
        game.Players.LocalPlayer.PlayerGui:FindFirstChild("Core_RollingButtons").Frame.Roll.Button.Visible = true
      end
    end
  end
  while task.wait() do
    pcall(task0)
  end
end)
if game.Lighting:FindFirstChild("Blur") then
       game.Lighting.Blur:Destroy()
end
